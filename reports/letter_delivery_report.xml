                                <div class="complement-details col-9">
                                    <div class="digital-stamp" style="background:#212f3d; color:#FFFFFF;">
                                        <span>Cadena Original del Complemento de Certificación Digital del SAT</span>
                                    </div>
                                    <div class="digital-stamp-content">
                                        <span class="nowrap" t-out="cfdi_values['cadena']"/>
                                    </div>
                                    <div class="digital-stamp" style="background:#212f3d; color:#FFFFFF;">
                                        <span>Sello Digital del CFDI</span>
                                    </div>
                                    <div class="digital-stamp-content">
                                        <span t-out="cfdi_values['sello']"/>
                                    </div>
                                    <div class="digital-stamp" style="background:#212f3d; color:#FFFFFF;">
                                        <span>Sello del SAT</span>
                                    </div>
                                    <div class="digital-stamp-content">
                                        <span t-out="cfdi_values['sello_sat']"/>
                                    </div>
                                    <div class="digital-stamp-content text-center">
                                        <strong>This document is a printed representation of a CFDI</strong>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </t>
                </t>
            </t>
        </template>
